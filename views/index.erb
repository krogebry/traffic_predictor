
<div id="options">
	<table cellspacing="0" cellpadding="0" style="width: 100%;">
	<tr>
		<td style="width: 25%;">
			<label for="highway">Highway</label>
			<select name="highway"><% RoadSystems.each do |highway| %>
				<option <%= (highway == 5 ? "selected" : "" ) %> value="<%= highway %>"><%= highway %></option>
			<% end %></select>
		</td>

		<td style="width: 50%;"><table cellspacing="0" cellpadding="0" style="width: 100%;height: 50px;">
		<tr>
			<td>Hour:</td>
			<td style="width: 25%;"><div id="hour_slider"></div></td>

		</tr><tr>
			<td>Minute:</td>
			<td style="width: 75%;"><div id="minute_slider"></div></td>

		</tr>
		</table></td>

		<td style="width: 25%;">
			<div id="tod"></div>
			<button id="btnUpdate">Get Prediction</button>
		</td>
	</tr>
	</table>

</div><!-- options -->

<div id="map_canvas"></div> 

